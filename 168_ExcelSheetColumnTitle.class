public class Solution {
    public String convertToTitle(int n) {
        //String result="";
         StringBuilder result = new StringBuilder();
        
        while(n>0){
            n--;
            //result=(char)('A'+n%26)+result;
            result.insert(0, (char)('A' + n % 26));
            n=n/26;
        }
        
        //return result;
        return result.toString();
    }
}

